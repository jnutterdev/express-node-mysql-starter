{{> breadcrumb}}
{{#each rows}}
<table class="table is-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
        </tr>
    </tbody>
</table>
{{/each}}